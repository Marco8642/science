local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Marco8642/science/main/ui%20libs2", true))()
local example = library:CreateWindow({
  text = "Hunting Season"
})
example:AddToggle("Freeze Animals", function(state)
getfenv().autofreeze = (state and true or false)
if getfenv().autofreeze == false then
    for i,v in pairs(workspace.Animals:GetDescendants()) do
        if v.Name == "HumanoidRootPart" and v.Anchored == true  then
       v.Anchored = false
       task.wait()
       end
       end
end
while getfenv().autofreeze do
    task.wait()
for i,v in pairs(workspace.Animals:GetDescendants()) do
 if v.Name == "HumanoidRootPart" and v.Anchored == false and getfenv().autofreeze then
v.Anchored = true
task.wait()
end
end
end
end)
example:AddToggle("Auto Harvest Dead Animals", function(state)
getfenv().dead = (state and true or false)
while getfenv().dead do
    task.wait()
for i,v in pairs(workspace.DeadAnimals:GetDescendants()) do
    if v.ClassName == "ProximityPrompt" and v.Enabled == true then
        repeat wait()
            if v.Parent ~= nil then
            game.Players.LocalPlayer.Character:MoveTo(v.Parent.Parent.Parent.Position)
            fireproximityprompt(v)
            end
        until v.Parent == nil or v.Enabled == false 
    end
end
end
end)
example:AddToggle("Aimbot", function(state)
    getfenv().getaim = (state and true or false)
    while getfenv().getaim do
        task.wait()
local aimat = nil
local distance = math.huge
for a,b in pairs(workspace.Animals:GetDescendants()) do
    if b.Name == "Brain"  then
local Dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - b.Position).magnitude
if Dist < distance then
distance = Dist
aimat = b
end
end
end
if aimat ~= nil then
    workspace.Camera.CFrame = CFrame.new(workspace.Camera.CFrame.Position,aimat.Position)
end
end
end)
example:AddToggle("Highlight Animals", function(state)
    getfenv().highlight = (state and true or false)
    if getfenv().highlight == false then
        for i,v in pairs(workspace.Animals:GetChildren()) do
            if v.ClassName == "Model" and v:FindFirstChild("Highlight") then
                v:FindFirstChild("Highlight"):Destroy()
                task.wait()
            end
        end
    end
    while getfenv().highlight do
    task.wait()
    for i,v in pairs(workspace.Animals:GetChildren()) do
    if v.ClassName == "Model" and not v:FindFirstChild("Highlight") and getfenv().highlight then
        Instance.new("Highlight",v)
    task.wait()
    end
    end
    end
end)
example:AddButton("Gun mod", function()
    for i,v in pairs(getgc(true)) do
        if type(v) == "table" and rawget(v,"MagazineCapacity") ~= nil then
    v.WalkSpeedPenalty = 0
    v.RunSpeedPenalty = 0
    v.LoadBetweenRounds = false
    v.VerticalRecoil = 0
    v.MagazineCapacity = math.huge
    end
    end
end)

