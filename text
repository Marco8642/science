for i,v in pairs(game:GetService("Workspace").Map:GetDescendants()) do
    if v.Name == "BoundingBox" and _G.test == true   and v.Parent:FindFirstChild("bValues") and v.Parent.bValues:FindFirstChild("Health") and v.Parent.bValues:FindFirstChild("Health").Value > v.Parent.bValues:FindFirstChild("MaxHealth").Value/2  and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.Position).Magnitude < _G.distance then
local location = v.Position
local hum = game.Players.LocalPlayer.Character.HumanoidRootPart
local x = location.X
local y = location.Y
local z = location.Z

                 game.Players.LocalPlayer.Character.ClientRunner.KaijuControlEvent:FireServer(_G.skillname)
if x < hum.Position.X then
    while x < hum.Position.X do
                game.Players.LocalPlayer.Character.ClientRunner.KaijuControlEvent:FireServer(_G.skillname)
        local CFrameEnd = CFrame.new(Vector3.new(hum.Position.X-_G.tp,hum.Position.Y,hum.Position.Z))
game.Players.LocalPlayer.Character.Parent.PrimaryPart = game.Players.LocalPlayer.Character.HumanoidRootPart
              local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(0.05, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0)

local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = game.Players.LocalPlayer.Character.Parent:GetPrimaryPartCFrame()
			
TweenValue.Changed:Connect(function()
	game.Players.LocalPlayer.Character.Parent:SetPrimaryPartCFrame(TweenValue.Value)
end)
			
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=CFrameEnd})
OnTween:Play()
 OnTween.Completed:Wait()
    end
end
if x > hum.Position.X then
    while x > hum.Position.X do
                game.Players.LocalPlayer.Character.ClientRunner.KaijuControlEvent:FireServer(_G.skillname)
    local CFrameEnd = CFrame.new(Vector3.new(hum.Position.X+_G.tp,hum.Position.Y,hum.Position.Z))
