warn("Anti afk running")
game:GetService("Players").LocalPlayer.Idled:connect(function()
warn("Anti afk ran")
game:GetService("VirtualUser"):CaptureController()
game:GetService("VirtualUser"):ClickButton2(Vector2.new())
end)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Marco8642/science/main/ui%20libs2"))()
local example = library:CreateWindow({
  text = "DEJP"
})
example:AddButton("Get Courier Job", function(state)
game:GetService("ReplicatedStorage").Job.General.Remotes.Client.SetJob:InvokeServer("CourierDriver")
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Job.CourierDriver.Start.Start.ProximityPrompt.Parent.CFrame
wait(0.5)
fireproximityprompt(workspace.Job.CourierDriver.Start.Start.ProximityPrompt)
end)
example:AddToggle("Auto farm[Courier]", function(state)
getfenv().test = (state and true or false)
while getfenv().test do
task.wait()
     local save = workspace.General.Waypoint.CFrame
    local plr = game.Players.LocalPlayer
    local chr = plr.Character
    local car = chr.Humanoid.SeatPart.Parent
    car.PrimaryPart = car.Body["#Weight"]
    getfenv().first  = true
    repeat task.wait()
    if save == workspace.General.Waypoint.CFrame and getfenv().first == true then
    car:PivotTo(car.PrimaryPart.CFrame+Vector3.new(0,1000,0))
    local dist = (car.PrimaryPart.Position- workspace.General.Waypoint.Position).magnitude
     local TweenService = game:GetService("TweenService")
    local TweenInfoToUse = TweenInfo.new(20, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 0, false, 0)
    
    local TweenValue = Instance.new("CFrameValue")
    TweenValue.Value = car:GetPrimaryPartCFrame()
                
    TweenValue.Changed:Connect(function()
        car:PivotTo(TweenValue.Value)
        car.PrimaryPart.Velocity = Vector3.new(0,0,0)
    end)
                
    local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=workspace.General.Waypoint.CFrame+Vector3.new(0,1000,0)})
    OnTween:Play()
     OnTween.Completed:Wait()
      getfenv().first = false
      elseif save == workspace.General.Waypoint.CFrame and getfenv().first == false then
    local dist = (car.PrimaryPart.Position- workspace.General.Waypoint.Position).magnitude
     local TweenService = game:GetService("TweenService")
    local TweenInfoToUse = TweenInfo.new(0, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 0, false, 0)
    
    local TweenValue = Instance.new("CFrameValue")
    TweenValue.Value = car:GetPrimaryPartCFrame()
                
    TweenValue.Changed:Connect(function()
        car:PivotTo(TweenValue.Value)
        car.PrimaryPart.Velocity = Vector3.new(0,0,0)
    end)
                
    local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=workspace.General.Waypoint.CFrame})
    OnTween:Play()
     OnTween.Completed:Wait()
     end
    until save ~= workspace.General.Waypoint.CFrame
     local timer = tick()
     repeat task.wait()
     car.PrimaryPart.Velocity = Vector3.new(0,0,0)
     until tick()-timer>5
    end
    end)
    example:AddToggle("Auto Open[Basic Box]", function(state)
    getfenv().open = (state and true or false)
    while getfenv().open do
game:GetService("ReplicatedStorage").Box.Remotes.Client.RollBox:FireServer("BasicBox")
task.wait(1)
end
end)
