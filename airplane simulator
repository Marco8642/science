local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Marco8642/science/main/ui%20libs2", true))()
local example = library:CreateWindow({
  text = "Airplane simulator"
})
example:AddToggle("Auto Tasks", function(state)
_G.auto = (state and true or false)
while _G.auto do
    wait()
    pcall(function()
if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
for i,v in pairs(game:GetService("Workspace").SpawnedVehicles:GetChildren()) do
    if v.ClassName == "Model" and game.Players.LocalPlayer:DistanceFromCharacter(v.PrimaryPart.Position) < 20  then
        for a,b in pairs(game:GetService("Players").LocalPlayer.Tasks:GetDescendants()) do
            if b.Name == "Destination" then
              local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(0, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0)

local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = v:GetPrimaryPartCFrame()
			
TweenValue.Changed:Connect(function()
	v:SetPrimaryPartCFrame(TweenValue.Value)
end)
			
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=game:GetService("Workspace").Destinations[b.Value].CFrame+Vector3.new(0,1,0)})
OnTween:Play()
 OnTween.Completed:Wait()
 wait(2)
local ohString1 = "Cessna Skyhawk"
local ohInstance2 = workspace.Spawners

game:GetService("ReplicatedStorage").SpawnVehicle:FireServer(ohString1, ohInstance2)
wait(2)
            elseif b.Name == "Airport" then
               local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(0, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0)

local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = v:GetPrimaryPartCFrame()
			
TweenValue.Changed:Connect(function()
	v:SetPrimaryPartCFrame(TweenValue.Value)
end)
			
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=game:GetService("Workspace").AirportDestinations[b.Value].CFrame+Vector3.new(0,1,0)})
OnTween:Play()
 OnTween.Completed:Wait()
 wait(2)
local ohString1 = "Cessna Skyhawk"
local ohInstance2 = workspace.Spawners

game:GetService("ReplicatedStorage").SpawnVehicle:FireServer(ohString1, ohInstance2)
wait(2)
 elseif b.Name == "Goal" and b.Parent.Type.Value == "Fly distance" then
                   local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0)

local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = v:GetPrimaryPartCFrame()
			
TweenValue.Changed:Connect(function()
	v:SetPrimaryPartCFrame(TweenValue.Value)
end)
			
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value= v.PrimaryPart.CFrame+Vector3.new(100009,1000,0)})
OnTween:Play()
 OnTween.Completed:Wait()
 local ohString1 = "Cessna Skyhawk"
local ohInstance2 = workspace.Spawners

game:GetService("ReplicatedStorage").SpawnVehicle:FireServer(ohString1, ohInstance2)
wait(2)
end
end
    end
end
elseif game.Players.LocalPlayer.Character.Humanoid.Sit ~= true then
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohString1 = "Cessna Skyhawk"
local ohInstance2 = workspace.Spawners

game:GetService("ReplicatedStorage").SpawnVehicle:FireServer(ohString1, ohInstance2)
wait(2)
end
end)
end
end)
local example = library:CreateWindow({
    text = "Credits"
  })
  example:AddLabel("MARCO POLO#3842",function()
  end)
